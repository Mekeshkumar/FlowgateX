# FlowGateX Environment Variables

# API Configuration
VITE_API_BASE_URL=http://localhost:5000/api
VITE_WEBSOCKET_URL=ws://localhost:5000

# Authentication
VITE_JWT_SECRET=your-jwt-secret-key
VITE_AUTH_TOKEN_KEY=flowgatex_auth_token

# Payment Gateway (Stripe/Razorpay)
VITE_STRIPE_PUBLIC_KEY=your-stripe-public-key
VITE_RAZORPAY_KEY_ID=your-razorpay-key-id

# IoT Configuration
VITE_IOT_GATEWAY_URL=http://localhost:8080
VITE_MQTT_BROKER_URL=mqtt://localhost:1883

# Analytics
VITE_GOOGLE_ANALYTICS_ID=UA-XXXXXXXXX-X

# Feature Flags
VITE_ENABLE_CHATBOT=true
VITE_ENABLE_IOT=true
VITE_ENABLE_CROWD_MONITORING=true

# Environment
VITE_APP_ENV=development
VITE_APP_NAME=FlowGateX
VITE_APP_VERSION=1.0.0
